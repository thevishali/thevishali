def factor(n):
    factors=[]
    for i in range(1,int(n** 0.5)+1):
        if n%i==0:
            factors.append(i)
            if n//i!=i:
                factors.append(n//i)
    return sorted(factors)
def find_p(n,p):
    factors=factor(n)
    if p<=len(factors):
        return factors[p-1]
    else:
        return 0
n=int(input())
p=int(input())
print(find_p(n,p))
