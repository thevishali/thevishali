from collections import defaultdict
def findRepeatedDnaSequences(s):
    if len(s) < 10:
        return []
    freq = defaultdict(int)
    result = set()
    for i in range(len(s) - 9):
        freq[s[i:i+10]] += 1
    for seq, freq_val in freq.items():
        if freq_val > 1:
            result.add(seq)
    return list(result)
def sortString(str):
    return ''.join(sorted(str))
     
# Driver code
s = input()
lst=findRepeatedDnaSequences(s)
str1=lst[0]
lst2=sortString(str1)
print(lst2)
